{
  "name": "wedding-site",
  "version": "1.0.0",
  "private": true,
  "description": "Website for my 2018 wedding",
  "scripts": {
    "tsc": "tsc --rootDir src",
    "webpack": "webpack --mode development",
    "webpack-prod": "webpack --mode production",
    "build": "yarn run tsc && yarn run webpack",
    "deploy": "yarn run tsc && yarn run webpack-prod && ts-node ./scripts/deploy.ts --prod",
    "local": "yarn run webpack && ts-node ./scripts/local.ts",
    "local-prod": "yarn run webpack-prod && ts-node ./scripts/local.ts"
  },
